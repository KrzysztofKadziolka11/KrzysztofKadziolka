    <script>

        const liczba=prompt("Podaj liczbe a:");
        const liczba1=prompt("Podaj liczbe b:");
        if(liczba/liczba1%2==0){
            document.write("Liczba jest podzielna");
        }
        else{
            document.write("Liczba jest niepodzielna");
        }

    </script>
